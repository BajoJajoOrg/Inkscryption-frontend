{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			":api": ["src/api"],
			":api/*": ["src/api/*"],
			":components": ["src/components"],
			":components/*": ["src/components/*"],
			":services": ["src/services"],
			":services/*": ["src/services/*"],
			":shared": ["src/shared"],
			":shared/*": ["src/shared/*"],
			":constants": ["src/constants"],
			":constants/*": ["src/constants/*"],
			":widgets": ["src/widgets"],
			":widgets/*": ["src/widgets/*"],
			":pages": ["src/pages"],
			":pages/*": ["src/pages/*"],
			":styles": ["src/styles"],
			":styles/*": ["src/styles/*"],
			":types": ["src/types"],
			":images": ["src/assets/images"],
			":images/*": ["src/assets/images/*"],
			":svg/*": ["src/assets/svg/*"],
			":svg": ["src/assets/svg"],
			":utils": ["src/utils"],
			":utils/*": ["src/utils/*"],
			":modules": ["src/modules"],
			":modules/*": ["src/modules/*"],
			":core": ["src/core"],
			":core/*": ["src/core/*"],
			":hooks": ["src/hooks"],
			":hooks/*": ["src/hooks/*"],
			"package.json": ["./package.json"],
			":lib": ["src/lib"],
			":lib/*": ["src/lib/*"]
		}
	}
}
